{{extend 'layout.html'}}

<div>
    <table>
        <tr>
            <td>UnitID</td>
            <td>UserID</td>
            <td>Access</td>
        </tr>
        {{while rows:}}
        <tr>
            <td>{{=rows.unit_id}}</td>
            <td>{{=row.user_id}}</td>
            <td>{{=row.access_type}}</td>
        </tr>
        {{pass}}
    </table>
</div>
